<schemalist>
	<schema gettext-domain="xmradio" id="com.timxx.xmradio" path="/apps/xmradio/">
		<key name="window-pos" type="(ii)">
			<default>(-1,-1)</default>
			<_summary>Window position</_summary>
			<_description>The last window position</_description>
		</key>
		<key name="theme" type="s">
			<default>'pure'</default>
			<_summary>UI theme</_summary>
			<_description>Set null or leave empty to use gtk theme</_description>
		</key>
		<key name="radio-url" type="s">
			<default>''</default>
			<_summary>radio url</_summary>
			<_description>Radio url</_description>
		</key>
		<key name="radio-name" type="s">
			<default>''</default>
			<_summary>radio name</_summary>
			<_description>radio name</_description>
		</key>
		<key name="auto-login" type="b">
			<default>true</default>
			<_summary>auto login</_summary>
			<_description>auto login</_description>
		</key>
		<key name="usr" type="s">
			<default>''</default>
			<_summary>user name</_summary>
			<_description>user name</_description>
		</key>
		<key name="pwd" type="s">
			<default>''</default>
			<_summary>password</_summary>
			<_description>user password</_description>
		</key>
	</schema>
	<schema id="com.timxx.xmradio.plugins" path="/apps/xmradio/plugins/">
    <key name="active-plugins" type="as">
      <default>['notification']</default>
      <_summary>Active plugins</_summary>
      <_description>List of active plugins.</_description>
    </key>
  </schema>
</schemalist>
